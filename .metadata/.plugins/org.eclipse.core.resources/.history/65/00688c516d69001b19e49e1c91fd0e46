package org.vishwa;

public class HelloWorld {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int x = -17;
		int y = 10;
		
		System.out.println("The Value of X is : " +(x) );
		System.out.println("The Value of X is : " +(-x) );
		
		System.out.println(y++ );

	}

}
